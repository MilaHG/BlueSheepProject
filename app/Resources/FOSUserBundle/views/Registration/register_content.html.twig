<h2>Inscription :</h2>
{% trans_default_domain 'FOSUserBundle' %}
{{ form_start(form, {'method': 'post', 'action': path('fos_user_registration_register'), 'attr': {'class': 'fos_user_registration_register'}}) }}

   <div class="form-group">
    <label for="exampleInputEmail1">Vous êtes ? : </label>
    {{ form_widget(form.role) }}
  </div>

  <div class="form-group">
    <label for="exampleInputEmail1">Civilité : </label>
    {{ form_widget(form.gender) }}
  </div>
  
  <div class="form-group">
    <label for="exampleInputEmail1">Email : </label>
    {{ form_widget(form.email) }}
  </div>
  
  <div class="form-group">
    <label for="exampleInputEmail1">Nom de connexion : </label>
    {{ form_widget(form.username) }}
  </div>
 
  <div class="form-group">
    <label for="exampleInputEmail1"></label>
   {{ form_widget(form.plainPassword) }}
  </div>
  
  <div class="form-group">
    <label for="exampleInputEmail1">Nom : </label>
   {{ form_widget(form.firstname) }}
  </div>
  
  <div class="form-group">
    <label for="exampleInputEmail1">Prenom : </label>
   {{ form_widget(form.lastname) }}
  </div>
  
  <div class="form-group">
    <label for="exampleInputEmail1">Date de naissance : </label>
    {{ form_widget(form.birthdate) }}
  </div>
  
  <div class="form-group">
    <label for="exampleInputEmail1">Adresse : </label>
    {{ form_widget(form.address) }}
  </div>
  
  <div class="form-group">
    <label for="exampleInputEmail1">Ville : </label>
    {{ form_widget(form.city) }}
  </div>
  
  <div class="form-group">
    <label for="exampleInputEmail1">Code postal : </label>
    {{ form_widget(form.zip) }}
  </div>
  
  <div class="form-group">
    <label for="exampleInputEmail1"> Avatar : </label>
    {{ form_widget(form.photo) }}
  </div>
 
    
    
    <div>
        <input  class="btn btn-primary" type="submit" value="{{ 'registration.submit'|trans }}" />
    </div>
{{ form_end(form) }}